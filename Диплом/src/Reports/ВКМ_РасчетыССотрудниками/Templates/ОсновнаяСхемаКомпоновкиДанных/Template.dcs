	КОНЕЦ КАК ПериодДействия,
	ЕСТЬNULL(ВТ_Сотрудники.ПериодРегистрации, ДАТАВРЕМЯ(1, 1, 1, 0, 0, 0)) КАК ПериодРегистрации,
	ВТ_Сотрудники.ОстатокНачало КАК ОстатокНачало,
	ВТ_Сотрудники.ОстатокКонец КАК ОстатокКонец,
	ВТ_Сотрудники.Удержано КАК Удержано,
	ВТ_Сотрудники.Выплачено
ИЗ
	ВТ_Сотрудники КАК ВТ_Сотрудники</query>
